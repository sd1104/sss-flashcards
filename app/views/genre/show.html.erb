<div class="container">
  <div class="Genre">
    <br><br>
    <h4>ジャンル　一覧</h4>
    <br>
    <ul class="Genre-lists">
      <% @genres.each do |g| %>
        <li class="list">
          <%= link_to genre_path(g) do %>
            <%= g.name %>
          <% end %>
        </li>
      <% end %>
    </ul>
  </div>
</div>